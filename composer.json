{
  "name": "naimeken/omnipay-masterpass",
  "type": "library",
  "description": "Masterpass gateway for Omnipay payment processing library",
  "homepage": "https://github.com/alegraio/omnipay-masterpass",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Omnipay\\Masterpass\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OmnipayTest\\Masterpass\\": "tests/"
    }
  },
  "require": {
    "php": "^7.3 || ^8.0",
    "omnipay/common": "^3",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-soap": "*",
    "ext-openssl": "*"
  },
  "require-dev": {
    "omnipay/tests": "^3",
    "squizlabs/php_codesniffer": "^3",
    "phpro/grumphp": "^0.18"
  },
  "prefer-stable": true
}
